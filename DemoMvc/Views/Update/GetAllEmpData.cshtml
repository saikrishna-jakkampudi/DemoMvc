@model DemoMvc.Models.EMPDATA
@{
    ViewBag.Title = "GetAllEmpData";
}

<h2>GetAllEmpData</h2>

@using (Html.BeginForm())
{
    <table>
        <tr>
            <td>Emp Number</td>
            <td>@Html.TextBoxFor(a => a.EMPNO)</td>
        </tr>
        <tr>
            <td>Emp Name</td>
            <td>@Html.TextBoxFor(a => a.ENAME)</td>
        </tr>
        <tr>
            <td>Job</td>
            <td>@Html.TextBoxFor(a => a.JOB)</td>
        </tr>
        <tr>
            <td>Manager</td>
            <td>@Html.TextBoxFor(a => a.MGR)</td>
        </tr>
        <tr>
            <td>Hiredate</td>
            <td>@Html.TextBoxFor(a => a.HIREDATE)</td>
        </tr>
        <tr>
            <td>Salary</td>
            <td>@Html.TextBoxFor(a => a.SAL)</td>
        </tr>
        <tr>
            <td>Commission</td>
            <td>@Html.TextBoxFor(a => a.COMM)</td>
        </tr>
        <tr>
            <td>Dept Number</td>
            <td>@Html.TextBoxFor(a => a.DEPTNO)</td>
        </tr>
    </table>
}
